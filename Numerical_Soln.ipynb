       "array([0.18628971, 0.73044999, 0.18628971, 0.73044999, 0.26817183,\n",
       "       0.45635623, 0.18628971, 0.18628971, 0.78543578, 0.59786916,\n",
       "       0.87415817, 0.1645494 , 0.67962072, 0.1645494 , 0.73044999,\n",
       "       0.18628971, 0.80159097, 0.49632382, 0.87415817, 0.78543578])"
      ]
     },
     "execution_count": 22,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "x = np.random.rand(M) # init all agents to [0,1]\n",
    "X = np.array([x]*M)\n",
    "mask = abs(X - X.T) < R\n",
    "A = np.zeros(M)\n",
    "for i in range(M):\n",
    "    A[i] = np.mean(x[mask[i]])"
